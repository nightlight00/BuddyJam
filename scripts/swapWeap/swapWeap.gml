function swapWeap(weaponSelect){
	var player_weap_map = playerWeapons[weaponSelect];
	weap_sprite = player_weap_map[? "weap_sprite"];
	weap_proj = player_weap_map[? "weap_proj"];
	weap_cooldown = player_weap_map[? "weap_cooldown"];
	weap_spread = player_weap_map[? "weap_spread"];
	weap_velocity = player_weap_map[? "weap_velocity"];
	weap_amount = player_weap_map[? "weap_amount"];
	weap_damage = player_weap_map[? "weap_damage"];
	weap_auto = player_weap_map[? "weap_auto"];
}